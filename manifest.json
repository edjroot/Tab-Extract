{
  "name": "Tab Extract",
  "version": "1.3.0",
  "description": "A simple & lightweight tool to reduce tab clutter, using just the address bar.",
  "minimum_chrome_version" : "9.0.0.0",
  "manifest_version": 2,
  "omnibox": { "keyword" : "ex" },
  "background": {
    "scripts": ["TabExtract.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "16": "icon_016.png",
      "32": "icon_032.png",
      "48": "icon_048.png",
      "128": "icon_128.png"
    }
  },
  "icons" : {
    "16" : "icon_016.png",
    "32" : "icon_032.png",
 	  "48" : "icon_048.png",
    "128": "icon_128.png"
  },
  "permissions": ["tabs"]
}

